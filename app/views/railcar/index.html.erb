<h1>Railcars</h1>

<table class="table">
  <thead class="thead-inverse">
    <tr>
      <th></th>
      <% @siding.positions.each do |position| %>
        <th>Position <%= position.number %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">Arrived</th>
      <% @siding.positions.each do |position| %>
        <th><%= position.arrived_at %></th>
      <% end %>
    </tr>
  </tbody>
</table>
